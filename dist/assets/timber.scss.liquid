.celebrity-gallery {

    /*
      these are based off the PSD, not what we are using for the max-width.
      That's ok, we're only using them for calculated, relative percentage values.
      */
    $desktopTotalWidth: 1350px;
    $desktopTotalHeight: 660px;
    $desktopMainWidth: 968px;
    $desktopSideWidth: 382px;
    $desktopMainLeftMargin: 115px;
    $desktopMainContentTop: 92px;
    $desktopThumbHeaderTop: 152px;
    $desktopThumbHeight: 220px;
    $desktopThumbLeftMargin: 16px;

    /* http://wemadeyoulook.at/en/blog/proportional-scaling-responsive-boxes-using-just-css/ */
    position: relative;
    height: 0;
    color: #fff;

    img {
        display: block;
        width: 100%;
        height: 100%;
        position: absolute;
        top: 0;
        left: 0;
        z-index: 0;
    }

    .main {
        position: absolute;
        left: 0;

        > div {
            @extend .clearfix;
            @include transition(0.2s opacity ease);

            position: absolute;
            width: 100%;
            height: 100%;
            opacity: 0;

            &.active {
                opacity: 1;
            }
        }
    }

    .content {
        position: absolute;
        z-index: 1;

        > * {
            margin: 20px 0;
            line-height: 1;
        }

        h3 {
            font-size: 2.5em;
            font-weight: 300;
        }

        p {
            font-size: 1.25em;
            font-weight: 100;
            letter-spacing: 0.0625em;
            text-shadow: 0 0 $colorOffWhite;
        }

        a {
            @extend %rsaquo;
            @extend %small-arial-text;

            text-transform: uppercase;
        }
    }

    /* Color schemes */
    .scheme-dark {

        &.thumb {
            text-shadow: 0 0 $colorOffWhite;            
        }

        .content {
            h3 {
                text-shadow: 0 0 white;
            }

            a {
                @extend %gold-hover-underline-link;
            }
        }
    }

    .scheme-light {

        h2, h3, p {
            color: $colorDarkGray;
        }
        
        &.thumb {
            text-shadow: 0 0 $colorOffWhite;

            h3 {
                color: white;
            }

            &:hover {
                text-shadow: 0 0 $colorDarkGray;

                h3 {
                    color: $colorDarkGray;
                }
            }

            &.active {
                text-shadow: 0 0 $colorDarkGray;

                h3 {
                    color: $colorDarkGray;
                }
            }
        }

        .content {
            h3 {
                text-shadow: 0 0 rgba(0,0,0,0.18);
            }

            a {
                @extend %darkGold-hover-underline-link;
            }
        }
    }

    .thumbs-block {
        position: absolute;
        background-color: #000;

        > a {
            @extend .clearfix;

            position: relative;
            display: block;
            height: 33.3333%;
            color: #fff;
            @include transition(0.2s all ease );
        }

        h3 {
            position: absolute;
            line-height: 1;
            margin: 0;
        }

        img {
            opacity: 0.6;
            @include transition(0.2s opacity ease );
        }

        a:hover img {
            opacity: 0.75;
        }

        .active, a:active, a.active:hover {
            img {
                opacity: 1;
            }
        }
    }

    /* most phones in portrait */
    @include at-query ($max, $small) {
        h2 {
            font-size: 35px;
        }

        .content {
            h3 {
                font-size: 25px;
                margin: 0 0 8px;
            }
            
            p {
                font-size: 15px;
                margin: 8px 0;
                font-weight: 100;
            }

            a {
                font-size: 12.5px;
                margin: 8px 0;
            }
        }

        .thumbs-block {
            h3 {
                display: none;
            }
        }
    }
    
    /* portrait tablet, landscape phones */
    @include at-query (null, $postSmall, $medium) {
        h2 {
            font-size: 2.25em;
            margin-top: 0;
        }

        .content {
            h3 {
                font-size: 1.75em;
                margin: 0 0 8px;
            }
            
            p {
                font-size: 12px;
                margin: 8px 0;
                font-weight: 300;
            }

            a {
                font-size: 11px;
                margin: 8px 0;
            }
        }

        .thumbs-block {
            h3 {
                display: none;
            }
        }
    }
    
    /* landscape tablet, small desktops */
    @include at-query (null, $postMedium, $large) {
        h2 {
            font-size: 2.75em;
        }

        .content {
            h3 {
                font-size: 2em;
                margin: 0 0 8px;
            }
            
            p {
                font-size: 13px;
                margin: 8px 0;
                font-weight: 300;
            }

            a {
                font-size: 13px;
                margin: 8px 0;
            }
        }

        .thumbs-block {
            h3 {
                font-size: 1.5em;
                font-weight: 300;
            }
        }
    }
    
    /* two col gallery layout */
    @include at-query( $min, $postSmall ){
        width: 100%;
        padding-bottom: percentage( $desktopTotalHeight / $desktopTotalWidth );
        
        h2 {
            width: 220px;
        }

        .main {
            width: percentage( $desktopMainWidth / $desktopTotalWidth );
            height: 100%;
        }

        .content {
            top: percentage( $desktopMainContentTop / $desktopTotalHeight );
            left: percentage( $desktopMainLeftMargin / $desktopMainWidth );
        }

        .thumbs-block {
            width: percentage($desktopSideWidth / $desktopTotalWidth);
            height: 100%;
            right: 0;

            > a {
                height: percentage(1 / 3);
            }

            h3 {
                bottom: percentage( 24px / $desktopThumbHeight );
                left: percentage( $desktopThumbLeftMargin / $desktopSideWidth );
                width: 126px;
            }
        }        
    }

    $smallTotalHeight: 568px;
    $smallTotalWidth: 320px;
    $smallMainHeight: 458px;
    $smallThumbBlockHeight: 110px;
    $smallLeftMargin: $gutter;
    $smallMainContentTop: 254px;

    /* single col gallery layout with thumbs on bottom row */
    @include at-query( $max, $small ){
        width: 100%;
        padding-bottom: percentage( $smallTotalHeight / $smallTotalWidth );
        
        .main {
            width: 100%;
            height: percentage( $smallMainHeight / $smallTotalHeight );
        }

        .content {
            top: percentage( $smallMainContentTop / $smallTotalHeight );
            left: percentage( $smallLeftMargin / $smallTotalWidth );
        }

        .thumbs-block {
            width: 100%;
            height: percentage($smallThumbBlockHeight / $smallTotalHeight);
            bottom: 0;
            left: 0;
            right: auto;

            > a {
                height: 100%;
                width: 33%;
                float: left;

                /* 33% width and then 34 on the first to avoid some rounding errors in some browsers. */
                &:first-child {
                    width: 34%;
                }
            }

            h3 {
                display: none;
            }
        }        
    }
}

