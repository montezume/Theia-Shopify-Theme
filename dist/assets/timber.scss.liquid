}


/*============================================================================
  #Cart Page
  ==============================================================================*/

%money-format {
    line-height: 1;
    word-spacing: -0.2em;
    white-space: nowrap;

    .currency {
        font-size: 0.5em;
        vertical-align: top;
    }
}

/* stying the '/cart' page */
.cart-page-layout {

    h2 {
        text-align: center;
    }

}

.shopping-cart {
    @extend .clearfix;

    h4 {
        font-weight: 300;
        margin: 0;
    }

    h6 {
        @extend %smallish-arial-text;

        font-weight: 700;
        text-transform: uppercase;
        color: $colorMidGray;
        margin-top: 0;
    }

    .cart-subtotal {
        @extend %money-format;

        font-family: $helveticaFontStack;
        font-weight: 300;
        color: $colorWarmDarkGray;
        padding-top: 4px;
        letter-spacing: -0.05em;
    }

    .checkout-button-row {
        text-align: center;
        border-bottom: 1px solid $colorLighterGray;
        padding-bottom: $gutter / 2;
        margin-bottom: $gutter;

        a {
            @extend %smallish-arial-text;
            @extend %gray-hover-gold-link;
            @extend %rsaquo;

            text-transform: uppercase;
            font-weight: 700;
        }

        button {
            margin-bottom: $gutter;
        }
    }

    .checkout-button {
        width: 100%;
        @extend %large-gold-arrow-button;
    }

    .update-cart-button {
        width: 100%;
        @extend %small-gray-button;
    }


    %order-info-item {
        color: $colorMidGray;

        .icon {
            @extend %icon-base;

            display: inline-block;
        }

        .label {
            @extend %smallish-arial-text;

            padding-top: 4px;
            text-transform: uppercase;
        }

        > span {
            vertical-align: middle;
            display: inline-block;
        }
    }

    .free-shipping {
        @extend %order-info-item;

        .icon {
            @extend .icon-gift;

            font-size: 18px;

            height: 18px;
        }
    }
    
    .shipped-by-date {
        @extend %order-info-item;

        margin-bottom: $gutter / 2;

        .icon {
            @extend .icon-calendar;

            height: 16px;
        }
    }

    .accepted-credit-cards {
        @extend .clearfix;

        > img {
            display: block;
            float: left;
            margin-left: 4px;

            &:first-of-type {
                margin: 0;
            }
        }
    }

    .cart-subtotal-row {
        margin-bottom: $gutter;
    }

    /* Desktop */
    @include at-query ($min, $postMedium) {
        h4 {
            font-size: 1.25em; /* ~20px */
        }

        /* Cart Layout */

        %cart-column {
            float: left;
            padding-top: $gutter;
            padding-bottom: $gutter / 2;
        }
        
        .main-col {
            @extend %cart-column;

            width: 912px;
            padding-left: $gutter;
            padding-right: $gutter * 2;
        }

        .side-col {
            @extend %cart-column;

            width: 240px;
            padding-left: ( $gutter * 2 ) - 1px;
            padding-right: $gutter;
            border-left: 1px solid $colorLighterGray;
        }

        .cart-terms-row {
            margin-top: $gutter * 2;
        }

        .cart-subtotal {
            font-size: 2.5em;
        }

    }

    @include at-query (null, $postMedium, $xLarge) {
        .main-col {
            width: 528px;
        }
    }

    /* Mobile */
    @include at-query ($max, $medium) {
        .main-col, .side-col {
            padding: 0 $gutter;
        }

        .main-col {
            border-bottom: 1px solid $colorLighterGray;
        }

        .side-col {
            margin-top: $gutter;
        }

        h4 {
            font-size: 1.5625em; /* ~25px */
            line-height: 1.32;
        }

        h6 {
            font-size: 10px;
        }

        .cart-subtotal-row {
            h6 {
                margin-bottom: $gutter / 2;
            }
        }

        .cart-subtotal {
            font-size: 1.875em;
        }

        .order-info-row,
        .accepted-credit-cards {
            display: none;
        }

        .checkout-button-row {
            padding-bottom: 0;
            border-bottom: 0;
        }
    }
}


