  ==============================================================================*/
.comment {
    margin-bottom: $gutter;

    & + & {
        border-top: 1px solid $borderColor;
        padding-top: $gutter;
    }
}


/*============================================================================
  #Notes and Form Feedback
  ==============================================================================*/
.note,
.errors {
    padding: $gutter/2;
    margin-bottom: $gutter/2;
    border: 1px solid transparent;

    ul,
    ol {
        margin-top: 0;
        margin-bottom: 0;
    }

    li:last-child {
        margin-bottom: 0;
    }

    p {
        margin-bottom: 0;
    }
}

.note {
    border-color: $borderColor;
}

.errors {
    ul {
        list-style: disc outside;
        margin-left: 20px;
    }
