        margin: 0;
    }

    /* Desktop */
    @include at-query ($min, $postSmall) {
        input[type="text"] {
            height: 30px;
        }
    }

    /* Mobile */
    @include at-query ($max, $small) {

        input[type="text"] {
            height: 44px;
        }
    }
}

.js--qty-adjuster {
    cursor: pointer;
    position: absolute;
    display: block;
    top: 0;
    bottom: 0;
    padding: 0;
    font-size: 12px;
    text-align: center;
    overflow: hidden;
    -webkit-user-select: none;
    -moz-user-select:    none;
    -ms-user-select:     none;
    -o-user-select:      none;
    user-select:         none;
    @include transition(all 0.2s ease-out);

    &:hover {
        background-color: $borderColor;
        color: #fff;
    }

    &:active {
        background-color: darken($borderColor, 10%);
    }

    /* Desktop */
    @include at-query ($min, $postSmall) {
        line-height: 30px;
        width: 30px;
    }

    /* Mobile */
    @include at-query ($max, $small) {
        line-height: 44px;
        width: 44px;
    }
}

.js--add {
    right: 0;
    // border-left: 1px solid $borderColor;
}

.js--minus {
    left: 0;
    // border-right: 1px solid $borderColor;
}

/*============================================================================
  #Quantity Selectors in the Ajax Cart
  ==============================================================================*/
.ajaxifyCart--qty {
    @extend .js-qty;
    margin: 0;

    .ajaxifyCart--is-loading & {
        opacity: 0.5;
        @include transition(none);
    }
}

.ajaxifyCart--num {
    @include transition(all 0.1s ease-out);

    .supports-no-touch & {
        border: 1px solid rgba(0,0,0,0);
    }
}

.ajaxifyCart--qty-adjuster {
    @extend .js--qty-adjuster;

    .cart-row:hover &,
    .ajaxifyCart--product:hover &,
    .supports-touch & {
        opacity: 1;
        @include transition(background-color 0.2s ease-out);
    }

    &:hover {
        background-color: $ajaxifyGreyDark;
    }

    &:active {
        background-color: darken($ajaxifyGreyDark, 10%);
    }

    // Turn off animation on initial hover
    .ajaxifyCart--product:hover &:hover {
        @include transition(none);
    }
}

.ajaxifyCart--add {
    @extend .js--add;
}

.ajaxifyCart--minus {
    @extend .js--minus;
}

/*============================================================================
  Ajaxify Drawer
  ==============================================================================*/
.ajax-drawer-container {
    background-color: $drawerBackgroundColor;
    height: 0px;
    overflow: hidden;
    @include perspective (1000px);
    @include transform-style(preserve-3d);
    @include transition(all 600ms cubic-bezier(0.47, 0.5, 0, 0.95));

    &.is-visible {
        @include transition(all 550ms cubic-bezier(0.47, 0, 0, 0.95));
    }

    .update-cart {
        display: none!important;
    }

    // Drawer colors and general styles
    .ajax-cart-container {
        margin: 0 auto;
        opacity: 0;
        overflow: hidden;
        background-color: $drawerBackgroundColor;
        color: color-control($drawerBackgroundColor);

        @include transform-origin(top);
        @include rotateX(-90deg);
    }

    &.is-visible .ajax-cart-container {
        opacity: 1;
        @include rotateX(0deg);
    }

    // Quantity selector colors in the drawer
    .js-qty {
        margin: 0 auto;
    }
}

.ajaxifyDrawer-caret {
    position: relative;
    display: block;
    height: 0;
    opacity: 0;
    overflow: hidden;

    .is-visible + & {
        opacity: 1;
        overflow: visible;
    }

    & > span {
        position: absolute;
        top: 0;
        left: 100%;
        display: block;
        width: 0;
        height: 0;
        border-left: 6px solid transparent;
        border-right: 6px solid transparent;
        border-top: 6px solid $drawerBackgroundColor;
        margin-left: -6px;
        opacity: 0;
        @include transform(translate(0,-12px));
        @include transition(all 0.25s ease-in-out 0.2s);
    }

    .is-visible + & > span {
        @include transform(translate(0,0));
        opacity: 1;
    }
}


/*============================================================================
  Carousel
  ============================================================================*/

// Default Variables

$slick-arrow-color: white !default;
$slick-dot-color: $colorWarmLightGray;
$slick-dot-color-active: $colorWarmDarkGray;
$slick-dot-size: 6px !default;
$opacity-default: .75;
$opacity-on-hover: 1;
$opacity-not-active: .25;

/* Slider */

.slick-slider {
    @include prefix('user-select', 'none');

    position: relative;
    display: block;
    -webkit-touch-callout: none;
    -ms-touch-action: none;
    -webkit-tap-highlight-color: transparent;
}

.slick-list {
    position: relative;
    overflow: hidden;
    display: block;
    margin: 0;
    padding: 0;

    &:focus {
        outline: none;
    }

    .slick-loading & {
        background: #fff;
    }

    &.dragging {
        cursor: pointer;
        cursor: hand;
    }
}

.slick-slider .slick-list,
.slick-track,
.slick-slide,
.slick-slide img {
    @include transform('translate3d(0, 0, 0)');
}

.slick-track {
    @extend .clearfix;

    position: relative;
    left: 0;
    top: 0;
    display: block;

    .slick-loading & {
        visibility: hidden;
    }
}

.slick-slide {
    float: left;
    height: 100%;
    min-height: 1px;

    img {
        display: block;
    }

    &.slick-loading img {
        display: none;
    }

    display: none;

    &.dragging img {
        pointer-events: none;
    }

    .slick-initialized & {
        display: block;
    }

    .slick-loading & {
        visibility: hidden;
    }

    .slick-vertical & {
        display: block;
        height: auto;
        border: 1px solid transparent;
    }
}

/* Arrows */

%carousel-arrow-base {
    @extend %icon-base;

    position: absolute;
    display: block;
    height: 20px;
    width: 20px;
    cursor: pointer;
    padding: 0;
    text-decoration: none;
    color: $colorLightGray;
    opacity: $opacity-default;
    z-index: 1;

    &:hover, &:focus {
        outline: none;
        background: transparent;
        color: $colorGold;

        &:before {
            opacity: $opacity-on-hover;
        }
    }

    &.slick-disabled:before {
        opacity: $opacity-not-active;
    }
}

/* Dots */

.slick-dots {
    $diameter: 18px;
    $radius: $diameter / 2;

    position: absolute;
    bottom: -54px;
    height: $diameter;
    list-style: none;
    display: block;
    text-align: center;
    padding: 0;
    margin: 0;
    width: 100%;
    z-index: 0;

    li {
        position: relative;
        display: inline-block;
        margin: 0 4px;
        padding: 0;
        cursor: pointer;
        vertical-align: top;

        a {
            border: 0;
            background: $slick-dot-color;
            display: block;
            height: $diameter;
            width: $diameter;
            outline: none;
            line-height: 1;
            font-size: 0;
            color: transparent;
            padding: 0;
            cursor: pointer;
            @include prefix( 'border-radius', $radius);            
            opacity: $opacity-not-active;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;

            &:hover, &:focus {
                outline: none;
                opacity: $opacity-on-hover;
            }
        }

        &.slick-active a {
            color: $slick-dot-color-active;
            opacity: $opacity-default;
        }
    }
}

