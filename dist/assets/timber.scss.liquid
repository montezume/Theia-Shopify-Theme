}

.product-image-thumbs-block {
    $thumbDesktopSize: 96px;
    $thumbDesktopMargin: $gutter / 4;
    $thumbMobileSize: 64px;
    $thumbMobileMargin: $gutter / 4;

    position: absolute;
    left: 0;

    a {
        display: block;
        border: 1px solid $colorLightGray;
        overflow: hidden;
        background: #fff;
    }

    img {
        display: block;
        width: 100%;
    }

    /* Desktop */
    @include at-query ($min, $postMedium) {
        top: $gutter * 2;

        li {
            margin: 0 0 $thumbDesktopMargin;
        }
    }

    @include at-query( $min, $postLarge ){
        width: $thumbDesktopSize;
        height: ($thumbDesktopSize * 6) + (5 * $thumbDesktopMargin);

        a {
            height: $thumbDesktopSize;
            width: $thumbDesktopSize;
        }
    }

    @include at-query( null, $postMedium, $large ){
        width: $thumbMobileSize;
        height: ($thumbMobileSize * 6) + (5 * $thumbMobileMargin);

        a {
            height: $thumbMobileSize;
            width: $thumbMobileSize;
        }
    }

    /* Mobile */
    @include at-query ($max, $medium) {
        bottom: $gutter;
        height: $thumbMobileSize;
        right: $gutter;
        overflow: hidden;
        text-align: center;

        li {
            margin: 0 $thumbMobileMargin/2;
            display: inline-block;
        }

        a {
            height: $thumbMobileSize;
            width: $thumbMobileSize;
        }
    }
}

%product-info-block-row {
    @extend .clearfix;

    /* Desktop */
    @include at-query ($min, $postMedium) {
        margin: 20px;
    }
    
    /* Mobile */
    @include at-query ($max, $medium) {
        margin: $gutter;
    }
}

.product-info-block {

    &.expanded {
        display: block;
    }

    .tablet-column-row,
    .tablet-column {
        @extend .clearfix;
    }

    /* Desktop */
    @include at-query ($min, $postMedium) {
        background: #fff;
        height: 639px;
        border: 1px solid $colorLightGray;

        /* expanded product description state */
        &.description-expanded {
            .product-purchase-block {
                display: none;
            }

            .product-meta-block {
                margin-bottom: 0;
                border-bottom: 0;
            }

            .product-description-block {
                .content {
                    height: 282px;
                    overflow-y: scroll;
                }
            }
        }
    }

    @include at-query( $min, $postLarge ){
        width: 448px;

    }

    @include at-query( null, $postMedium, $large ){
        width: 326px;

        /* expanded product description state */
        &.description-expanded {
            .product-description-block .content {
                height: 358px;
            }
        }
    }

    @include at-query( null, $postSmall, $medium ){
        .tablet-column {
            float: right;
            width: 50%;
        }
    }

    /* Mobile */
    @include at-query ($max, $medium) {
        display: none;
        
        background: $colorPaleGray;
        border-bottom: 1px solid $colorLightGray;
    }
}

.product-meta-block {
    @extend %product-info-block-row;
    @extend .clearfix;

    h2 {
        font-size: 1.8em;
        font-weight: 300;
        color: $colorWarmDarkGray;
        line-height: 1;
        margin: 0 0 11px;
    }

    .product-price {
        font-family: $helveticaFontStack;
        color: $colorWarmLightGray;
        line-height: 1;
    }

    .product-style-number {
        @extend %smallish-arial-text;

        text-transform: uppercase;
        color: $colorLightGray;
        font-weight: 700;
        line-height: 1;
    }

    /* Desktop */
    @include at-query ($min, $postSmall) {
        border-bottom: 1px solid $colorLightestGray;
        padding: 0 0 $gutter/2;

        .product-price {
            font-size: 1.5em;
            margin-bottom: 6px;
        }
    }

    @include at-query( null, $postMedium, $large ){
        h2 {
            font-size: 1.6em;
        }
    }

    @include at-query( null, $postSmall, $medium ){
        h2 {
            font-size: 1.75em;
        }
    }
    
    /* Mobile */
    @include at-query ($max, $small) {
        h2 {
            display: none;
        }

        > div {
            float: left;
            width: 50%;
        }

        .product-price {
            font-size: 1.25em;
        }

        .product-style-number {
            text-align: right;
        }
    }
}

.product-purchase-block {
    @extend %product-info-block-row;

    button {
        @extend %large-gold-arrow-button;

        margin-right: $gutter;
    }

    .add-to-wishlist-link {
        @extend %gray-underline-hover-gold-link;
        @extend %rsaquo;
        @extend %smallish-arial-text;

        text-transform: uppercase;
    }

    /* Desktop */
    @include at-query ($min, $postMedium) {
        margin-bottom: 22px;
    }
    
    @include at-query (null, $postMedium, $large) {

        .add-to-wishlist-link {
            display: block;
            margin: $gutter 0;
        }
    }

    /* Mobile */
    @include at-query ($max, $medium) {
        button {
            display: block;
            width: 100%;
            font-size: 1.25em;
            height: 44px;

            .icon {
                font-size: 0.625em;
                top: -1px;
            }
        }

        .add-to-wishlist-link {
            display: block;
            margin: $gutter 0;
        }
    }
}

.product-options-block {
    @extend .clearfix;

    .selector-wrapper {
        @extend .clearfix;

        margin-top: 10px;

        &:first-child {
            margin-top: 0;
        }

        label {
            margin: 0;

            &:after {
                content: ':';
            }
        }

        select {
            @extend %small-arial-text;

            background: $colorLightestGray;
            padding: 0 8px;
            color: $colorWarmGray;
            margin: 0 $gutter 0 0;

            &:focus {
                outline: none;
            }
        }
    }
    
    /* Desktop */
    @include at-query ($min, $postMedium) {
        margin-bottom: 27px;

        label, legend {
            width: 72px;
        }

        .selector-wrapper {
            $height: 30px;

            label, select {
                float: left;
            }

            label {
                line-height: $height;

                &:after {
                    content: ':';
                }
            }

            select {
                width: 170px;
                height: $height;
                line-height: $height;
            }
        }
    }

    @include at-query( null, $postMedium, $large ){
        .selector-wrapper {
            label,
            legend {
                width: 64px;
            }

            select {
                width: 125px;
            }
        }
    }
    
    /* Mobile */
    @include at-query ($max, $medium) {
        .selector-wrapper {
            $height: 44px;
            margin-bottom: $gutter;

            label {
                margin-bottom: $gutter / 2;

                &:after {
                    content: ':';
                }
            }

            select {
                height: $height;
                line-height: $height;
            }
        }
    }
}

.product-offer-block {
    color: $colorWarmLightGray;
    margin: 18px 0 0;

    span {
        vertical-align: middle;
    }

    .icon {
        @extend %icon-base;
        @extend .icon-gift;

        font-size: 18px;
    }

    .text {
        @extend %smallish-arial-text;

        font-weight: 700;
        text-transform: uppercase;
    }

    /* Desktop */
    @include at-query ($min, $postMedium) {
        
    }
    
    /* Mobile */
    @include at-query ($max, $medium) {
        display: none;
    }
}

.product-share-block {
    @extend %product-info-block-row;

    border-top: 1px solid $colorLightestGray;
    border-bottom: 1px solid $colorLightestGray;
    padding: 12px 0 14px;
    margin-top: 0px;

    > div {
        display: inline-block;
        line-height: 1;
        vertical-align: middle;
    }

    .label {
        @extend %smallish-arial-text;

        font-weight: 700;
        text-transform: uppercase;
        color: $colorWarmLightGray;
        padding-top: 2px;
    }

    .share-buttons {
        height: 20px;
        width: 354px;

        > * {
            display: inline-block !important;
            vertical-align: top !important;
        }

        .twitter-share-button {
            width: 80px !important;
        }
    }

    /* Desktop */
    @include at-query( $min, $postMedium ){
        
    }

    @include at-query( null, $postMedium, $large ){
        display: none;
    }
    
    /* Mobile */
    @include at-query ($max, $large) {
        .label {
            display: none;
        }
    }
}

.product-description-block {
    @extend %product-info-block-row;

    padding-bottom: 44px; /* room for the more details expand link */
    position: relative;
    margin-bottom: 23px;
    margin-top: 0;
    padding-top: 19px;

    h6 {
        @extend %smallish-arial-text;

        text-transform: uppercase;
        margin-top: 0;
        margin-bottom: 9px;
        font-weight: 700;
        color: $colorWarmLightGray;
    }

    p {
        @extend %smaller-helvetica-text;

        line-height: (23/14);
        color: $colorWarmGray;
    }

    .content {
        height: 64px; /* height of the header and 3 lines of text. */
        overflow-y: hidden;
    }



    /* Desktop */
    @include at-query ($min, $postMedium) {
        position: absolute;
        bottom: 0;
        left: 0;
    }
    
    @include at-query( null, $postMedium, $large ){
        border-top: 1px solid $colorLightestGray;
    }

    /* Mobile */
    @include at-query ($max, $medium) {
        padding-bottom: 0;

        p {
            font-size: 0.75em;
        }

        .content {
            height: auto;
        }

        .expand {
            display: none;
        }
    }
}

.product-description-expand-link {
    @extend %gray-hover-gold-link;

    position: absolute;
    bottom: 0;
    left: 0;
    color: $colorWarmLightGray;

    .label {
        @extend %smaller-arial-text;

        font-weight: 700;
        text-transform: uppercase;
        text-decoration: underline;
    }

    .collapse-text {
        display: none;
    }

    .description-expanded & {
        .collapse-text {
            display: inline;
        }

        .expand-text{
            display: none;
        }
    }

    .icon {
        @extend %icon-base;

        font-size: 11px;
        color: $colorDarkGray;

        &.up {
            @extend .icon-arrow-up-small;
        }

        &.down {
            @extend .icon-arrow-down-small;
        }
    }

    /* Mobile */
    @include at-query ($max, $medium) {
        display: none;
    }
}




