}

th {
    font-weight: bold;
}

th, td {
    text-align: left;
    padding: $gutter/2;
    border: 1px solid $borderColor;
}


/*============================================================================
  #OOCSS Media Object
  //  - http://www.stubbornella.org/content/2010/06/25/the-media-object-saves-hundreds-of-lines-of-code/
  ==============================================================================*/
.media,
.media-flex {
    overflow: hidden;
    _overflow: visible;
    zoom: 1;
}

.media-img {
    float: left;
    margin-right: $gutter;
}

.media-img-right {
    float: right;
    margin-left: $gutter;
}

.media-img img,
.media-img-right img {
    display: block;
}


/*============================================================================
  #Images and Iframes
  ==============================================================================*/
img.auto,
.grid-item img,
.grid-item iframe {
    max-width: 100%;
    height: auto;
}


/*============================================================================
  #Forms
  ==============================================================================*/

button,
input[type="text"],
input[type="search"],
input[type="password"],
input[type="email"],
input[type="file"],
input[type="number"],
input[type="tel"],
input[type="submit"],
textarea {
    -webkit-appearance: none;
    -moz-appearance: none;

    &:focus {
        outline: none;
    }
}

input,
textarea,
select,
fieldset {
    max-width: 100%;

    &.input-full {
        width: 100%;
    }
}

input,
select,
textarea {
    @include prefix( 'border-radius', 0 ); /* resets the user agent border radius in webkit*/
}

input {
    display: inline-block;
    width: auto;
}

button,
input[type="submit"] {
    cursor: pointer;
}

input[type="submit"] {
    @extend .btn;
}

/*================ Input width  ================*/
input[type="text"],
input[type="password"],
input[type="email"],
input[type="file"],
input[type="number"],
input[type="tel"],
textarea,
select {
    width: 100%;
    max-width: 100%;
    display: block;
    margin-bottom: 22px;
    border: 1px solid $colorWarmLightGray;
    background: $colorPaleGray;
    padding: ( $gutter / 2 ) $gutter;
    line-height: 1.625;

    &:focus {
    /* border: 1px solid darken($borderColor, 10%); */
    }

    &[disabled],
    &.disabled {
        cursor: default;
        background-color: $disabledGrey;
        border-color: $disabledBorder;
    }
}

input.half {
    width: 49%;
    margin-right: 2%;
    float: left;

    & + input.half {
        margin-right: 0;
    }
}


textarea {
    min-height: 100px;
}

/* custom checkboxs and radios */

input[type="checkbox"],
input[type="radio"] {
    display: inline;
    margin: 0;

    /* position labels following to be inline */
    & + label {
        display: inline-block;
        text-transform: none;
        font-family: $helveticaFontStack;
        color: $colorWarmGray;
    }

    /* Desktop */
    @include at-query ($min, $postSmall) {
        & + label {
            font-size: 16px;
        }
    }

    /* Mobile */
    @include at-query ($max, $small) {
        & + label {
            font-size: 15px;
        }
    }
}

%checkbox-radio-icon {
    /* hide custom checkbox styles from browers that do not support css3 psuedo classes */
    /* http://code.stephenmorley.org/html-and-css/hiding-rules-with-the-negation-pseudo-class/ */
    &:not(old) {
        display: none;

        /* http://code.stephenmorley.org/html-and-css/styling-checkboxes-and-radio-buttons/ */
        & + label {
            @extend %icon-base;
            @extend .icon-box-unchecked;

            /* ensure correct positioning of the icon, if the block is center aligned. */
            text-align: left;

            &:before {
                display: inline-block !important;
                color: $colorGold;
                position: relative;
                top: 1px;
            }
        }

        &:checked + label {
            @extend .icon-box-checked;

            color: $colorGold;
        }

        /* Desktop */
        @include at-query ($min, $postSmall) {
            & + label:before {
                width: 18px;
                font-size: 16px;
            }
        }

        /* Mobile */
        @include at-query ($max, $small) {
            & + label:before {
                width: 24px;
                font-size: 14px;
            }
        }
    }
}

/* fabric/color swatch radio block */
%checkbox-radio-swatch {
    &:not(old) {
        display: none;

        & + label {
            margin: 0;
            height: 32px;
            width: 32px;
        }

        &:not(:checked) + label {
            border: 3px solid white;
        }

        &:checked + label {
            @extend %darkGray-outline-block-medium;
        }
    }
}

.swatch {
    @extend .clearfix;

    $height: 32px;

    padding: 0;
    height: $height;

    legend {
        display: block;
        margin: 0;
        line-height: $height;
    }

    .swatch-element {
        width: 32px;
        margin: 0 2px 0 0;
        background: pink;
    }

    .swatch-element, legend {
        float: left;
        height: 100%;
    }

    .tooltip {
        display: none;
    }

    img {
        display: block;
    }

    input[type="radio"], input[type="checkbox"] {
        @extend %checkbox-radio-swatch;
    }
}


.contact-form {
    button {
        @extend %medium-gold-rsaquo-button;
        width: 288px;
    }

    input[type="radio"], input[type="checkbox"] {
        @extend %checkbox-radio-icon;
    }

    /* Desktop */
    @include at-query ($min, $postSmall) {
        .newsletter-signup-row,
        .submit-button-row {
            margin: ( $gutter * 1.5 ) 0 ( $gutter * 1.75 );
            text-align: center;

            label {
                margin: 0;
            }
        }

        .message {
            height: 325px;
        }
    }

    /* Mobile */
    @include at-query ($max, $small) {
        .submit-button-row {
            margin: ($gutter * 1.25) $gutter 0;
        }

        .newsletter-signup-row {
            margin: ($gutter * 1.25) $gutter;
            line-height: 1;

            label {
                margin: 0;
            }
        }

        .message {
            height: 88px;
        }
    }
}

.contact-form-title,
.contact-form-sender-type {
    @extend .clearfix;

    label {
        display: block;
        float: left;
    }

    /* Desktop */
    @include at-query ($min, $postSmall) {
        legend {
            display: none;
        }
    }

    /* Mobile */
    @include at-query ($max, $small) {
        margin-bottom: 6px;

        legend {
            margin: 8px 0;
            float: left;
            width: 100%;
        }

        label {
            margin: 6px 0 6px;
        }
    }
}

.contact-form-title {
    /* Desktop */
    @include at-query ($min, $postSmall) {
        label {
            width: 25%;
        }
    }

    /* Mobile */
    @include at-query ($max, $small) {
        label {
            width: 50%;
        }
    }
}

.contact-form-sender-type {
    /* Desktop */
    @include at-query ($min, $postSmall) {
        label {
            width: percentage(1/3);
        }
    }

    /* Mobile */
    @include at-query ($max, $small) {
        label {
            width: 50%;
        }
    }
}

select {
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    background: {
        image: url("data:image/svg+xml;base64,"); // svg base64 encoded arrow
        repeat: no-repeat;
        position: right 10px center;
        color: #fff;
    }
    padding-right: 28px;
    text-indent: 0.01px;
    text-overflow: '';

    /*================ Hide the svg arrow in IE9 and below ================*/
    .ie9 &,
    .lt-ie9 & {
        padding-right: 10px;
        background-image: none;
    }
}

select::-ms-expand {
    display: none;
}

/*================ Form labels ================*/
label,
legend {
    @extend %smallish-arial-text;

    display: block;
    margin: ($gutter / 2 ) 0 10px;
    font-weight: 700;
    text-transform: uppercase;
    color: $colorWarmLightGray;
    line-height: 1;

    .meta {
        font-weight: 400;
    }

    &.inline {
        display: inline;
    }

    .form-horizontal &.hidden-label,
    &.hidden-label {
        height: 0;
        width: 0;
        margin-bottom: 0;
        overflow: hidden;

        .ie9 &,
        .lt-ie9 & {
            height: auto;
            margin-bottom: 2px;
            overflow: visible;
        }
    }
}

/*================ We don't want the same label treatment for checkboxes/radios ================*/
input[type="checkbox"] + label,
input[type="radio"] + label {
    font-weight: normal;
}

label[for] {
    cursor: pointer;
}

.label-hint {
    color: #999;
}

/*================ Horizontal Form ================*/
form.form-horizontal,
.form-horizontal {
    margin-bottom: 0;

    input[type="text"],
    input[type="search"],
    input[type="password"],
    input[type="email"],
    input[type="file"],
    input[type="number"],
    input[type="tel"],
    textarea,
    select,
    label {
        display: inline-block;
        margin-bottom: 0;
        width: auto;
    }
}

/*================ Error styles ================*/
input[type="text"],
input[type="search"],
input[type="password"],
input[type="email"],
input[type="file"],
input[type="number"],
input[type="tel"],
textarea {
    &.error {
        border-color: $errorRed;
        background-color: $errorRedBg;
        color: $errorRed;
    }
}

label.error {
    color: $errorRed;
}


/*================ Input Group ================*/
.input-group {
    position: relative;
    display: table;
    border-collapse: separate;

    input {
        // Nasty Firefox hack for inputs http://davidwalsh.name/firefox-buttons
        &::-moz-focus-inner {
            border: 0;
            padding: 0;
            margin-top: -1px;
            margin-bottom: -1px;
        }
    }
}

.input-group-field,
.input-group-btn {
    display: table-cell;
    vertical-align: middle;
    margin: 0;
}

.input-group .btn,
.input-group .input-group-field {
    height: 37px;
}

.input-group .input-group-field {
    width: 100%;
    margin-bottom: 0;
}

.input-group-btn {
    position: relative;
    white-space: nowrap;
    width: 1%;
    padding: 0;
}

.form-spacer {
    margin: 0 $gutter/2;
}

/*============================================================================
