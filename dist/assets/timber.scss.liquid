@mixin latest-collections-link-size($height, $width, $clipWidth){
    height: $height;
    width: $width;

    &:before {
        background-size: $width $height;
    }

    img {
        height: $height;
        width: $width;
        clip: rect($clipWidth, $width - $clipWidth, $height - $clipWidth, $clipWidth);
    }
}

.latest-collections-link {
    @extend %white-hover-gray-link;
    @extend %small-arial-text;

    $height: 117px;
    $width: 173px;

    text-transform: uppercase;
    display: block;
    position: relative;

    height: $height;
    width: $width;

    /* contains the background image for the transparent border outline */
    &:before {
        content: '';
        position: absolute;
        top: 0;
        bottom: 0;
        width: 100%;

        background-color: #fff;
        background-position: left top;
        background-repeat: no-repeat;
        background-size: $width $height;

        transition: all 0.4s;
    }

    span {
        @extend %rsaquo;

        line-height: 1.15;
        position: absolute;
        top: 46px;
        left: 20px;
        width: 82px;
        white-space: normal;
    }

    img {
        height: $height;
        width: $width;
        position: absolute;
        top: 0;
        left: 0;
        display: block;

        filter: url("data:image/svg+xml;utf8,<svg xmlns=\'http://www.w3.org/2000/svg\'><filter id=\'desaturate\'><feColorMatrix type=\'saturate\' values=\'0.43\'/></filter></svg>#desaturate"); /* Firefox 10+, Firefox on Android */
        filter: gray; /* IE6-9 */
        -webkit-filter: grayscale(43%); /* Chrome 19+, Safari 6+, Safari 6+ iOS */

        @include prefix('transition', 'all 0.4s ease');

        clip: rect(5px, $width - 5px, $height - 5px, 5px);
    }

    .overlay {
        @include prefix('box-shadow', 'inset 0px 0px 76px 0px rgba(50, 50, 50, 0.75)');

        background: rgba(black, 0.20);
        position: absolute;
        top: 0;
        bottom: 0;
        width: 100%;
        @include prefix('transition', 'box-shadow 0.4s, opacity 0.4s');
    }

    &:hover {
        img {
            filter: none;
            -webkit-filter: none;
        }

        .overlay {
            opacity: 0;
            @include prefix('box-shadow', 'inset 0px 0px 0px 0px rgba(50, 50, 50, 0.75)');
        }

        &:before {
            opacity: 0.6;
        }
    }

    /* Small, most phones in landscape */
    @include at-query( $max, $small ){
        @include latest-collections-link-size($height: 59px, $width: 90px, $clipWidth: 2px);

        .overlay {
            @include prefix('box-shadow', 'inset 0px 0px 40px 0px rgba(50, 50, 50, 0.75)');
        }

        span {
            top: 23px;
            left: 9px;
            font-size: 6px;
            width: 37px;
        }
    }
    
    /* Medium, portrait tablet */
    @include at-query (null, $postSmall, $medium) {
        @include latest-collections-link-size($height: 94px, $width: 138px, $clipWidth: 4px);

        .overlay {
            @include prefix('box-shadow', 'inset 0px 0px 40px 0px rgba(50, 50, 50, 0.75)');
        }

        span {
            top: 23px;
            left: 9px;
            font-size: 6px;
            width: 37px;
        }        
    }
