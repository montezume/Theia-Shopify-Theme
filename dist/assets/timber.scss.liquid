/*================================================================================
  # Layouts
  ================================================================================*/

%wrapper-base {
    @extend .clearfix;

    position: relative;
    margin: 0 auto;
}

.wrapper-14-col {
    @extend %wrapper-base;

    max-width: $siteWidth14col;
}

.wrapper {
    @extend %wrapper-base;

    max-width: $siteWidth;
}

.wrapper-10-col {
    @extend %wrapper-base;

    @include at-query($max, $preLarge){
        margin: 0 $gutter;
    }

    /* Desktop */
    @include at-query ($min, $large) {
        max-width: $siteWidth10Col;
    }
}

.wrapper-9-col {
    @extend %wrapper-base;

    max-width: $siteWidth9Col;
}

.wrapper-8-col {
    @extend %wrapper-base;

    $breakPoint: $siteWidth8Col + ($gutter * 2);
    $preBreakPoint: $breakPoint - 1px;

    @include at-query($max, $preBreakPoint){
        margin: 0 $gutter;
    }

    /* Desktop */
    @include at-query ($min, $breakPoint) {
        max-width: $siteWidth8Col;
    }
}

%row-base {
    @extend .clearfix;

    position: relative;

    /* Desktop */
    @include at-query ($min, $postMedium) {
        .divider {
            height: 100%;
            width: 0;
            position: absolute;
            border-right: 1px solid $colorLightestGray;
        }

        .col {
            float: left;
        }
    }

    @include at-query( $max, $small ){
        .divider {
            margin: ($gutter * 2) $gutter;
        }
    }

    @include at-query( null, $postSmall, $medium ){
        .divider {
            max-width: $small - ($gutter * 2);
            margin: ($gutter * 2) auto;
        }
    }

    /* Mobile */
    @include at-query ($max, $medium) {
        .divider {
            width: auto;
            height: 0;
            position: relative;
            border-bottom: 1px solid $colorLightestGray;
        }

        .col {
            width: auto;
            max-width: $small;
            float: none;
            margin: 0 auto;
            /* padding: { */
            /*     left: $gutter; */
            /*     right: $gutter; */
            /* } */
        }
    }
}

.row-2-col {
    @extend %row-base;

   /* Desktop */
    @include at-query ($min, $postMedium) {
        .divider {
            left: 50%;
        }

        .col {
            width: 50%;
            padding: ( $gutter / 2 ) $gutter ( $gutter / 2 ) ( $gutter * 3 );

            &:first-of-type {
                padding-right: $gutter * 3;
                padding-left: $gutter;
            }
        }
    }
}

.row-3-col {
    @extend %row-base;

    /* Desktop */
    @include at-query ($min, $postMedium) {
        .divider {
            left: percentage(1/3);

            & ~ .divider {
                left: percentage(2/3);
            }
        }

        .col {
            padding: ( $gutter / 2 ) ( $gutter * 2 ) ( $gutter / 2 );
            width: percentage(1/3);
        }
    }

    /* Mobile */
    @include at-query ($max, $medium) {
    }
}

%inline-list-base {
    ul, li {
        margin: 0;
    }

    li {
        display: inline-block;
        vertical-align: top;
    }
}

%nav-menu-base {
    @extend %inline-list-base;

    @include at-query ($min, $postLarge) {
        ul {
            height: 21px;
        }
    }

    .site-footer & {
        @include at-query ($max, $large) {
            ul {
                @include clearfix();
            }

            li {
                display: block;
                float: left;
                padding: 0 $gutter/2;
            }
        }

        @include at-query ($max, $medium) {
            li {
                width: 50%;
            }
        }

        @include at-query(null, $postMedium, $large){
            li {
                width: percentage(1/3);
            }
        }
    }
}


/*============================================================================
  #Basic Styles
  ==============================================================================*/
html,
body {
    background-color: $colorBody;
    position: relative;
}


/*============================================================================
  #Helper Classes
  ==============================================================================*/
.left { float: left; }
.right { float: right; }
.text-center { text-align: center; }
.text-right { text-align: right; }
.hidden { display: none; }
.show { display: initial !important; }

/* Desktop */
@include at-query ($min, $postSmall) {
    .hidden-on-desktop {
        display: none !important;
    }
}

/* Mobile */
@include at-query ($max, $small) {
    .hidden-on-mobile {
        display: none !important;
    }
}

.display-table {
    display: table;
}

.display-table-cell {
    display: table-cell;
    vertical-align: middle;
}


/*============================================================================
  #Typography
  ==============================================================================*/
body,
input,
textarea,
button,
select {
    font-size: 1em;
    font-family: $arialFontStack;
    line-height: 1.4375; // ~23px with a font size of 16px
    font-weight: 400;
    color: $colorWarmGray;
    -webkit-font-smoothing: antialiased;
    -webkit-text-size-adjust: 100%;

    @include placeholder {
        line-height: 1.4375; // ~23px with a font size of 16px
        font-family: $arialFontStack;
        color: $colorMidGray;
    }
}

/* h1 tags have been excluded from this list. The h1 tag is used for the site header. */

h2, h3, h4, h5, h6,
.h2, .h3, .h4, .h5, .h6 {
    display: block;
    font-family: $headerFontStack;
    font-weight: $headerFontWeight;
    margin: 0.5em 0;
    line-height: 1;

    a {
        text-decoration: none;
    }
}

h2 a, h3 a, h4 a, h5 a, h6 a,
.h2 a, .h3 a, .h4 a, .h5 a, .h6 a { font-weight: inherit; }

/*============================================================================
  Pixel to EM conversion tool
  - http://pxtoem.com/
  ==============================================================================*/

h2, .h2 {
    font-size: 3.75em; // 60px
    /* portrait tablet, landscape phones, landscape tablet, small desktops */
    @include at-query (null, $postSmall, $large) {
        font-size: 3.25em; // 52px
    }
}   
h3, .h3 { font-size: 2.25em; }    // 36px
h4, .h4 { font-size: 1.5em; }     // 24px
h5, .h5 { font-size: 1.143em; }   // ? get these values from the PSD
h6, .h6 { font-size: 1em; }       // ?

p {
    margin: 0 0 $gutter/2 0;

    a {
        @extend %gray-underline-hover-gold-link;
    }
}


@include at-query ($max, $small) {

    p {
        font-size: 0.75em;
        margin: 0 0 $gutter/2 0;
    }

    h2, .h2 { font-size: 2.1875em; }  // 35px
    h3, .h3 { font-size: 2.25em; }    // 36px
    h4, .h4 { font-size: 1.5em; }     // 24px
    h5, .h5 { font-size: 1.143em; }   // ? get these values from the PSD
    h6, .h6 { font-size: 1em; }       // ?
}


p img { margin: 0; }

em { font-style: italic; }
strong { font-weight: bold; }
small { font-size: 80%; }
sup {
    position: relative;
    top: -0.5em;
    font-size: 60%;
}

/*================ Blockquotes ================*/
blockquote {
    font-size: 1.125em;
    line-height: 1.45;
    font-style: italic;
    margin: 0 0 $gutter;
    padding: $gutter/2 $gutter;
    border-left: 1px solid $borderColor;

    p {
        margin-bottom: 0;

        & + cite {
            margin-top: $gutter/2;
        }
    }

    cite {
        display: block;
        font-size: 0.75em;

        &:before {
            content: "\2014 \0020";
        }
    }
}

hr {
    clear: both;
    border-top: solid $borderColor;
    border-width: 1px 0 0;

    &.hr-4-col {
        max-width: 416px;
    }

    &.hr-8-col {
        max-width: $siteWidth8Col - $gridGutter;
    }

    /* Desktop */
    @include at-query ($min, $postSmall) {
        margin: 0 auto;
    }

    /* Mobile */
    @include at-query ($max, $small) {
        margin: 0 $gutter;
    }
}

/*================ Section Headers ================*/
.section-header {
    margin-bottom: $gutter/2;
}

@include at-query ($min, $large) {
    .section-header {
        display: table;
        width: 100%;
    }

    .section-header--left {
        display: table-cell;
        vertical-align: middle;
        margin-bottom: 0;

        h1, h2, h3, h4,
        .h1, .h2, .h3, .h4 {
            margin-bottom: 0;
        }
    }

    .section-header--right {
        display: table-cell;
        vertical-align: middle;
        text-align: right;
        width: 325px;

        @include at-query ($max, $medium) {
            margin-bottom: $gutter;
        }
    }
}

.section-header--right {
    @include at-query ($max, $medium) {
        margin-top: -0.4em; // don't exceed h1 margin-bottom
    }

    .form-horizontal,
    .collection-view {
        display: inline-block;
        vertical-align: middle;
    }

    @include at-query ($min, $postSmall) {
        label + select,
        .collection-view {
            margin-left: $gutter/2;
        }
    }
}

.collection-view {
    display: inline-block;
    border: 1px solid $borderColor;
    padding: 11px;
    height: 37px; // same as form elements

    /*================ We use SVG icons, so simply hide this in oldIE ================*/
    .lt-ie9 & {
        display: none;
    }

    /*================ Only show on larger screens ================*/
    @include at-query ($min, $large) {
        display: inline-block;
    }

    a {
        display: block;
        float: left;
        opacity: 0.7;
        cursor: pointer;

        & + a {
            margin-left: 10px;
        }

        &.collection-view--active {
            opacity: 1;
            cursor: default;
        }
    }

    img {
        display: block;
        opacity: inherit;
    }
}

/*============================================================================
  #Rich Text Editor
  ==============================================================================*/
.rte {
    margin-bottom: $gutter/2;

    // Add some top margin to headers from the rich text editor
    h1, h2, h3, h4, h5, h6 {
        margin-top: 2em;

        &:first-child {
            margin-top: 0;
        }
    }

    & > div {
        margin-bottom: $gutter/2;
    }

    ul, ol {
        margin-left: 35px;
    }

    ul {
        @extend ul.disc;
    }

    li {
        margin-bottom: 0.4em;
    }
}

