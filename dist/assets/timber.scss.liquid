%hero-edge-fade-base {

    /* gradient fade at edges of image */
    &:before, &:after {
        content: '';
        height: 100%;
        display: block;
        position: absolute;
        top: 0px;
    }
}
@mixin heroEdgeFade( $fadeColor, $width: 99px, $offset: $width ){
    @extend %hero-edge-fade-base;

    &:before, &:after {
        width: $width;
    }

    &:before {
        @include gradient(rtl, transparent, $fadeColor, $fadeColor);

        left: -$offset;
    }

    &:after {
        @include gradient(ltr, transparent, $fadeColor, $fadeColor);

        right: -$offset;
    }
}


%outline-block-base {
    position: relative;
    z-index: 0;

    /* outer border */
    border-style: solid;

    /* inner border */
    &:before {
        content: '';
        position: absolute;
        z-index: -1;
        border-style: solid;
        border-color: #fff;
    }
}

%gold-outline-block-base {
    @extend %outline-block-base;

    background: $colorGold;

    /* outer border */
    border-color: $colorGold;
}

%gray-outline-block-base {
    @extend %outline-block-base;

    background: $colorGray;

    /* outer border */
    border-color: $colorGray;
}

@mixin outlineBlockBorderWidths( $innerWidth, $outerWidth ){
    /* outer border */
    border-width: $outerWidth + $innerWidth;

    /* inner border */
    &:before {
        top: -$innerWidth;
        right: -$innerWidth;
        bottom: -$innerWidth;
        left: -$innerWidth;
        border-width: $innerWidth;
    }
}

%gold-outline-block-large {
    @extend %gold-outline-block-base;
    @include outlineBlockBorderWidths( 5px, 2px );
}

%gold-outline-block-small {
    @extend %gold-outline-block-base;
    @include outlineBlockBorderWidths( 1px, 1px );
}

%gray-outline-block-small {
    @extend %gray-outline-block-base;
    @include outlineBlockBorderWidths( 1px, 1px );
}

%darkGray-outline-block-medium {
    @extend %outline-block-base;
    @include outlineBlockBorderWidths( 2px, 1px );
}

%social-icon-base {
    @extend %icon-base;

    position: absolute;
    border-style: solid;
    text-align: center;

    @include at-query( $min, $postSmall ){
        $desktopCircumfrence: 3.375em;

        @include prefix( 'border-radius', $desktopCircumfrence / 2 );

        height: $desktopCircumfrence;
        width: $desktopCircumfrence;
        border-width: 0.15626em;
    }

    @include at-query( $max, $small ){
        $smallCircumfrence: 2.66em;

        @include prefix( 'border-radius', $smallCircumfrence / 2 );

        height: $smallCircumfrence;
        width: $smallCircumfrence;
        border-width: 0.125em;
    }
}


/*================ Force an input/button to look like a text link ================*/
.text-link {
    display: inline;
    border: 0 none;
    background: none;
    padding: 0;
    margin: 0;
}

/*============================================================================
  #Lists
  ==============================================================================*/
ul, ol { margin-bottom: 1em; }
ul { list-style: none outside; }
ol { list-style: decimal; }
ol, ul.square, ul.disc { margin-left: 20px; }
ul.square { list-style: square outside; }
ul.disc { list-style: disc outside; }
ol.alpha { list-style: lower-alpha outside; }
ul ul, ul ol,
ol ol, ol ul { margin: 4px 0 5px 20px; }
li { margin-bottom: 0.25em; }
.inline-list li {
