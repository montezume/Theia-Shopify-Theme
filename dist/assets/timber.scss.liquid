/* heros */

%hero-base {
    position: relative;
    color: #fff;

    .wrapper {
        height: 100%;
    }

    /* White image mask */
    &:before, &:after {
        content: '';
        height: 0px;
        width: 50%;
        display: block;
        position: absolute;
        bottom: 0px;
        border-top: 0;
        border-bottom: {
            style: solid;
            color: #fff;
        }
        z-index: 1000;
    }

    &:before {
        left: 0px;
        border-left: 0;
        border-right: {
            style: solid;
            color: transparent;
        }
    }

    &:after {
        right: 0px;
        border-right: 0;
        border-left: {
            style: solid;
            color: transparent;
        }
    }

    /* Desktop */
    @include at-query ($min, $postSmall) {
        &:before, &:after {
            border-bottom-width: 38px;
        }

        &:before {
            border-right-width: 48px;
        }

        &:after {
            border-left-width: 48px;
        }
    }

    /* Mobile */
    @include at-query ($max, $small) {
        &:before, &:after {
            border-bottom-width: 20px;
        }

        &:before {
            border-right-width: 20px;
        }

        &:after {
            border-left-width: 20px;
        }
    }
}

