$smallSocialIconRatio: (2.66/3.375);

@mixin socialIconResponsiveSizes( $iconSize, $paddingTop, $paddingRight: false, $paddingLeft: false ){
    @include at-query( $min, $postSmall ){
        padding-top: $paddingTop;

        @if $paddingRight {
            padding-right: $paddingRight;
        }

        @if $paddingLeft {
            padding-left: $paddingLeft;
        }

        &:before {
            font-size: $iconSize
        }
    }

    @include at-query( $max, $small ){
        padding-top: $paddingTop * $smallSocialIconRatio;
        @if $paddingRight {
            padding-right: $paddingRight * $smallSocialIconRatio;
        }

        @if $paddingLeft {
            padding-left: $paddingLeft * $smallSocialIconRatio;
        }

        &:before {
            font-size: $iconSize * $smallSocialIconRatio;
        }
    }
}

.pinterest-icon {
    @extend %social-icon-base;
    @extend .icon-pinterest-p;

    @include socialIconResponsiveSizes( $iconSize: 2.5em, $paddingTop: 0.625em, $paddingRight: 0.125em );
}

.twitter-icon {
    @extend %social-icon-base;
    @extend .icon-twitter;

    @include socialIconResponsiveSizes( $iconSize: 1.9375em, $paddingTop: 0.5em, $paddingLeft: 0.125em );
}

.instagram-icon {
    @extend %social-icon-base;
    @extend .icon-instagram;

    @include socialIconResponsiveSizes( $iconSize: 1.5625em, $paddingTop: 0.75em );
}

.tumblr-icon {
    @extend %social-icon-base;
    @extend .icon-tumblr;

    @include socialIconResponsiveSizes( $iconSize: 2.0625em, $paddingTop: 0.375em, $paddingLeft: 0.0625em );
}

.facebook-icon {
    @extend %social-icon-base;
    @extend .icon-facebook-f;

    @include socialIconResponsiveSizes( $iconSize: 2.125em, $paddingTop: 0.4375em, $paddingRight: 0.0625em );
}

/*============================================================================
  #Pagination
  ==============================================================================*/
.pagination-block {
    @extend .clearfix;

    position: relative;
    margin-bottom: 0;
    border-top: 1px solid $borderColor;
    border-bottom: 1px solid $borderColor;

    > li {
        display: inline-block;
        margin: 0;

        > a,
        > span {
            @extend %gray-hover-gold-link;
            @extend %small-mid-arial-text;

            display: inline-block;
            position: relative;
            padding: 11px 5px 9px;
            margin-left: -1px;
            line-height: 1.42;
            text-transform: uppercase;
            font-weight: 700;
        }

        /* Previous Link */
        &:first-child > a,
        &:first-child > span {
            margin-left: 0;
            padding-left: $gutter * 3;
            padding-right: $gutter;
        }

        /* Next Link */
        &:last-child > a,
        &:last-child > span {
            padding-left: $gutter;
            padding-right: $gutter * 3;
        }
    }

    > .active {
        > a,
        > span,
        > a:hover,
        > span:hover,
        > a:focus,
        > span:focus {
            z-index: 2;
            color: $colorGold;
            cursor: default;
        }
    }

    > .disabled {
        > span,
        > a,
        > a:hover,
        > a:focus {
            color: #999999;
            cursor: not-allowed;
        }
    }

    /* Desktop */
    @include at-query ($min, $postSmall) {
        display: inline-block;
    }

    /* Mobile */
    @include at-query ($max, $small) {
        > li {

            &:first-child,
            &:last-child {
                position: absolute;
                top: 0;
            }

            &:first-child {
                left: 0;
            }

            &:last-child {
                right: 0;
            }

            > a,
            > span {
                padding: 11px 5px 9px;
                font-size: 15px;
            }

            /* Previous Link */
            &:first-child > a,
            &:first-child > span {
                @extend %prev-next-link-base;

                margin-left: 0;
                padding-left: 0;
                padding-right: $gutter;

                /* hide the 2nd half of the word previous. */
                > span {
                    display: none;
                }

            }

            /* Next Link */
            &:last-child > a,
            &:last-child > span {
                @extend %prev-next-link-base;

                padding-left: $gutter;
                padding-right: 0;
            }
        }

        > .active {
            > a,
            > span,
            > a:hover,
            > span:hover,
            > a:focus,
            > span:focus {
                z-index: 2;
                color: $colorGold;
                cursor: default;
            }
        }

        > .disabled {
            > span,
            > a,
            > a:hover,
            > a:focus {
                color: #999999;
                cursor: not-allowed;
            }
        }
    }
}



/*============================================================================
  #Site Header
  ==============================================================================*/

/*================ Dropdowns ================*/
.site-nav--dropdown {
    display: none;
    position: absolute;
    left: 0;
    z-index: 5;

    .supports-no-touch .site-nav--has-dropdown:hover &,
    .site-nav--has-dropdown.nav-hover &,
    .nav-focus + & {
        display: block;
    }

    a {
        background-color: $colorNav;

        &:hover,
        &:active,
        &:focus {
            background-color: darken($colorNav, 10%);
        }
    }
}


/*============================================================================
  #Breadcrumbs
  ==============================================================================*/
.breadcrumb-block {
    @include overflowEllipsis();

    background: $colorLightestGray;
    border-bottom: 1px solid $colorLightGray;

    a,
    span {
        @extend %smallish-arial-text;

        display: inline-block;
        font-weight: 700;
    }

    a {
        @extend %rsaquo;
        @extend %warmGray-hover-underline-link;
    }

    span {
        color: $colorGold;
    }
    
    /* Desktop */
    @include at-query ($min, $postSmall) {
        height: $gutter * 2.25;
        padding: 2px ( $gutter * 1.25 ) 0;
        line-height: $gutter * 2;
    }

    /* Mobile */
    @include at-query ($max, $small) {
        // border-top: 1px solid $colorLightGray;
        height: 35px;
        padding: 0 $gutter;

        a,
        span {
            font-size: 0.625em;
            line-height: 33px;
        }
    }
}


/*============================================================================
  #Product Page
  ==============================================================================*/

.product-detail-hero {
    background: white;
    border-bottom: 1px solid $colorLighterGray;
    border-top: 1px solid $colorLighterGray;    

    .wrapper {
        height: 100%;
    }

    @include at-query( $min, $postMedium ){
        height: 692px;

        .product-gallery-block,
        .product-info-block {
            position: absolute;
        }

        .product-gallery-block {
            left: $gutter;
            top: 0;
            z-index: 0;
        }

        .product-info-block {
            right: $gutter;
            top: $gutter * 2;
            z-index: 1;
        }
    }

    /* Desktop */
    @include at-query ($min, $postLarge) {

        .product-gallery-block {
            right: 368px;
        }

    }

    @include at-query( null, $postMedium, $large ){

        .product-gallery-block {
            right: 278px;
        }

    }
}

.product-mobile-header-block {
