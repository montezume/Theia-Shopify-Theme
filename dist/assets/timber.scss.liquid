/* must be included within the avatar img selector block */
@mixin customerAvatarSize( $radius ) {
    height: $radius * 2;
    width: $radius * 2;
    @include prefix(border-radius, $radius);
}

/* styles shared between the desktop header and mobile side nav */
@mixin customerLoginLink() {
    img, #{$iconSelector}, .label {
        display: block;
        float: left;
    }

    img, #{$iconSelector} {
        margin-right: $gutter/3;
    }

    #{$iconSelector} {
        height: 1em;
        width: 1em;
    }

    /* used for the avatar */
    img {
        position: relative;
    }

    .label {
        @include overflowEllipsis();
    }
}

.customer-menu-link {
    @extend %gray-hover-gold-link;
    @include clearfix();

    display: block;
    position: relative;

    .site-header & {

        .label {
            @extend %small-arial-text;
        }

        /* Desktop */
        @include at-query ($min, $xLarge) {
            @include customerLoginLink();
            
            padding: 3px $gutter/2 2px;

            /* used for the avatar */
            img {
                @include customerAvatarSize( 11px );

                top: -2px;
                border: 2px solid $colorWarmGray;
            }

            .label {
                padding-top: 1px;
                line-height: 15px;
                max-width: 105px;
            }
        }

        /* Mobile */
        @include at-query ($max, $preXLarge) {
            height: 22px;
            width: 22px;
            border: 1px solid $colorGray;
            @include prefix(border-radius, 2px);
            text-align: center;

            #{$iconSelector} {
                height: 1em;
                width: 1em;
                font-size: 0.75em;
                display: block;
                margin: 4px auto;

                &:before {
                    display: block;
                }
            }

            .label {
                display: none;
            }
        }
    }

    .mobile-side-nav & {
        @include customerLoginLink();

        margin: 0 $gutter;
        padding: 13px 0;

        .label {
            @extend %smallish-arial-text;
        }
        
        /* used for the avatar */
        img {
            @include customerAvatarSize( 8px );

            border: 1px solid $colorWarmGray;
        }
    }
}

.cart-count-link {

    #{$iconSelector} {
        position: relative;
    }

    .count {
        @extend %smaller-arial-text;
        @include prefix(border-radius, 2px);
        @include transform('scale(0)');
        @include transition();

        opacity: 0;
        display: block;
        position: absolute;
        top: -10px;
        left: 4px;
        background-color: $colorGold;
        color: #fff;
        height: 16px;
        width: 16px;
        padding-top: 1px;
        line-height: 15px;
        text-align: center;

        &:before {
            content: '';
            display: block;
            border: 1px solid #fff;
            height: 14px;
            width: 14px;
            position: absolute;
            top: 1px;
            left: 1px;
            @include prefix(border-radius, 1px);
        }

        &.positive-count {
            @include transform('scale(1)');

            opacity: 1;
        }
    }
}

