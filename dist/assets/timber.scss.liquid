address {
    @extend %small-mid-arial-text;

    a {
        @extend %gold-hover-underline-link;
    }

    %address-item-base {
        @extend .clearfix;
        @extend %icon-base;

        position: relative;
        padding-left: 20px;
        margin: ( $gutter / 2 ) 0;
        line-height: 1.4285714285714286;

        /* icon */
        &:before {
            position: absolute;
            top: 3px;
            left: 0;
        }
    }

    .email {
        @extend %address-item-base;
        @extend .icon-stamp;

        &:before {
            color: $colorGold;
        }
    }

    .address {
        @extend %address-item-base;
        @extend .icon-map-pin;
    }

    .phone {
        @extend %address-item-base;
        @extend .icon-telephone-handset;
    }
}

/* Header */

.site-header {
    /* Using a mixin here since this is shared between media queries */
    @mixin header-column() {
        @include verticalCenterChildElements();
        @include box-sizing( content-box );

        position: relative;
        display: block;
        float: left;
    }

    .layout-theme &,
    .layout-customer-account &{
        .wrapper > div {
            @extend %column-base;
        }

        /* Desktop */
        @include at-query ($min, $xLarge) {
            padding: $gutter 0;
            height: $desktopHeaderHeight;

            .wrapper {
                position: relative;
                @extend .clearfix;
            }

            %header-column {
                @include header-column();

                height: $desktopHeaderHeight - $gridGutter;
                margin: 0 $gutter;
            }

            .logo-column {
                @extend %header-column;
            }

            .main-menu-column {
                @extend %header-column;
                width: 432px;
                text-align: right;

                ul {
                    padding-top: 2px;
                }
            }

            .customer-menu-column {
                @extend %header-column;

                text-align: right;
                width: 218px;
            }

            .search-column {
                @extend %header-column;

                width: 220px;
            }
        }

        /* Mobile */
        @include at-query ($max, $preXLarge) {
            padding: $gutter/2 0;

            height: $mobileHeaderHeight;

            .wrapper {
                @extend .clearfix;

                position: relative;
                margin: 0 $gutter;
            }

            %header-column {
                @include header-column();

                height: $mobileHeaderHeight - $gridGutter/2;
            }

            .logo-column {
                @extend %header-column;
            }

            .customer-menu-column {
                @extend  %header-column;
                float: right;
            }

            .main-menu-column,
            .search-column {
                display: none;
            }
        }
    }

    .layout-simplified & {
        padding: $gutter * 2 0 $gutter;

        /* center all child elements */
        > * {
            margin-left: auto;
            margin-right: auto;
        }

        @include at-query ($max, $small) {
            padding: ( $gutter * 1.5 ) 0 ( $gutter * 2 )
        }
    }
}

/* Main Content Layout */

/* This contains all the blocks that are not the header, footer, cart, or slide out side bar. */
.site-content {
    @extend .clearfix;
    display: block;

    .layout-simplified & {
        text-align: center;
        overflow-y: visible;
    }
}

/* Just a wrapper for hooking onto the page content and styling it. */
.page-content {
    @extend .wrapper;
}

/* footer */

.site-footer {
    @extend .wrapper-10-col;

    /* Desktop */
    @include at-query ($min, $postLarge) {
        .first-column {
            @include verticalCenterChildElements();
        }
    }

    /* Mobile */
    @include at-query ($max, $large) {
        margin: 0;
        
        > div {
            display: block;
            padding: 0 $gutter;
        }
    }

    .layout-theme &,
    .layout-customer-account & {
        padding: $gutter * 2 0;

        /* Desktop */
        @include at-query ($min, $postLarge) {

            border-top: 1px solid $borderColor;

            > div {
                @include verticalCenterChildElements();
                float: left;
            }

            .first-column {
                width: 672px;
            }

            .last-column {
                width: 288px;
                text-align: right;
            }
        }

        /* Mobile */
        @include at-query ($max, $large) {
            padding: 0;

            .first-column,
            .last-column {
                border-top: 1px solid $borderColor;
                margin: 0 16px;
                padding: 20px 0;
            }

            .last-column {
                li {
                    width: 100%;
                    text-align: right;
                    padding: 0;
                }
            }
        }
    }

    .layout-simplified & {
        /* Desktop */
        @include at-query ($min, $postSmall) {
            @include verticalCenterChildElements();

            padding: $gutter*2 0;
            text-align: center;

            .first-column {
                height: 40px;
                border-right: 1px solid $colorLightestGray;
            }

            .last-column {
